{
  "manifest_version": 2,

  "name": "Make America Dad Jokes Again",
  "description": "Improve your browsing experience",
  "version": "1.0",

   "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["dadjokes.js"],
      "run_at": "document_idle"
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
