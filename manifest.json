{
  "manifest_version": 2,

  "name": "Make America Dad Jokes Again",
  "description": "Improve your browsing experience",
  "version": "1.0",

   "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["madja.js"]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "madja.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
